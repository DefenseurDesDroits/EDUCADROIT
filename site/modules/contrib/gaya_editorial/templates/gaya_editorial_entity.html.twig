{#
/**
 * @file gaya_editorial_entity.html.twig
 * Default theme implementation to present Gaya editorial entity data.
 *
 * This template is used when viewing Gaya editorial entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_gaya_editorial_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('gaya_editorial_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
